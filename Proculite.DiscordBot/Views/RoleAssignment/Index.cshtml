@using Proculite.DiscordBot.Services
@inject RoleAssignmentService RoleAssignmentService

<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Guild name</th>
      <th scope="col">Message</th>
      <th scope="col">Select only one role</th>
    </tr>
  </thead>
  <tbody>
    @foreach (AssignRoleModel assignRole in RoleAssignmentService.GetAllAssignRoleMessages())
    {
        <tr>
          <th scope="row">@(assignRole.Id)</th>
          <td>@(assignRole.GuildName)</td>
          <td>@(assignRole.MessageContent)</td>
          <td>@(assignRole.ChooseOne ? "✅️" : "❌")</td>
        </tr>
    }
  </tbody>
</table>